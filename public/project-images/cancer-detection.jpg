<svg width="800" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad7" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#a8edea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fed6e3;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="400" fill="url(#grad7)"/>
  <text x="400" y="120" font-family="Arial, sans-serif" font-size="32" font-weight="bold" text-anchor="middle" fill="#2d3748">Cancer Detection</text>
  <text x="400" y="150" font-family="Arial, sans-serif" font-size="18" text-anchor="middle" fill="#4a5568">with Convolutional Neural Networks</text>
  
  <!-- Medical scan representation -->
  <rect x="150" y="180" width="200" height="150" fill="rgba(45, 55, 72, 0.1)" stroke="#4a5568" stroke-width="2"/>
  
  <!-- Scan patterns -->
  <circle cx="200" cy="220" r="8" fill="rgba(66, 153, 225, 0.6)"/>
  <circle cx="250" cy="240" r="6" fill="rgba(66, 153, 225, 0.4)"/>
  <circle cx="220" cy="260" r="10" fill="rgba(66, 153, 225, 0.8)"/>
  <circle cx="280" cy="280" r="7" fill="rgba(66, 153, 225, 0.5)"/>
  <circle cx="180" cy="300" r="9" fill="rgba(66, 153, 225, 0.7)"/>
  
  <!-- CNN layers -->
  <rect x="400" y="200" width="80" height="60" fill="rgba(72, 187, 120, 0.8)" rx="5"/>
  <text x="440" y="235" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="white">CNN</text>
  
  <rect x="500" y="200" width="80" height="60" fill="rgba(237, 137, 54, 0.8)" rx="5"/>
  <text x="540" y="235" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="white">MLP</text>
  
  <rect x="600" y="200" width="80" height="60" fill="rgba(245, 101, 101, 0.8)" rx="5"/>
  <text x="640" y="235" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="white">Output</text>
  
  <!-- Connecting arrows -->
  <line x1="350" y1="230" x2="400" y2="230" stroke="#4a5568" stroke-width="3"/>
  <polygon points="390,225 400,230 390,235" fill="#4a5568"/>
  
  <line x1="480" y1="230" x2="500" y2="230" stroke="#4a5568" stroke-width="3"/>
  <polygon points="490,225 500,230 490,235" fill="#4a5568"/>
  
  <line x1="580" y1="230" x2="600" y2="230" stroke="#4a5568" stroke-width="3"/>
  <polygon points="590,225 600,230 590,235" fill="#4a5568"/>
  
  <!-- Medical cross -->
  <rect x="400" y="300" width="80" height="15" fill="#e53e3e" rx="7"/>
  <rect x="437" y="263" width="15" height="80" fill="#e53e3e" rx="7"/>
  
  <text x="400" y="380" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#4a5568">AI-Powered Medical Diagnosis</text>
</svg>
